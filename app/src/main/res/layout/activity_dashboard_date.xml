<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/bkg10"
    android:layout_height="match_parent"
    tools:context="com.manujindal.stichme.DashboardDate">

    <GridLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|center_horizontal"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp">

        <TextView
            android:id="@+id/tablehead10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="1"
            android:fontFamily="sans-serif-smallcaps"
            android:text="Order Date"
            android:textColor="#000000"
            android:textSize="15dp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tablehead20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="2"
            android:fontFamily="sans-serif-smallcaps"
            android:text="Trial Date"
            android:textColor="#000000"
            android:textSize="15dp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tablehead30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="3"
            android:fontFamily="sans-serif-smallcaps"
            android:text="Delivery Date"
            android:textColor="#000000"
            android:textSize="15dp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tablehead01"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="0"
            android:fontFamily="sans-serif-smallcaps"
            android:gravity="center_vertical|center_horizontal"
            android:text="TextView"
            android:textColor="#000000"
            android:textSize="25dp" />

        <TextView
            android:id="@+id/tablehead02"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_row="0"
            android:fontFamily="sans-serif-smallcaps"
            android:gravity="center_vertical|center_horizontal"
            android:text="TextView"
            android:textColor="#000000"
            android:textSize="25dp" />

        <TextView
            android:id="@+id/tablehead03"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:layout_row="0"
            android:fontFamily="sans-serif-smallcaps"
            android:gravity="center_vertical|center_horizontal"
            android:text="TextView"
            android:textColor="#000000"
            android:textSize="25dp" />

        <ScrollView
            android:layout_width="368dp"
            android:layout_height="match_parent"
            android:layout_column="0"
            android:layout_columnSpan="4"
            android:layout_row="1"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ExpandableListView
                    android:id="@+id/tablelist11"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="0"
                    android:layout_row="0"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist12"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="1"
                    android:layout_row="0"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist13"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="2"
                    android:layout_row="0"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist21"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="0"
                    android:layout_row="1"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist22"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="1"
                    android:layout_row="1"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist23"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="2"
                    android:layout_row="1"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist31"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="0"
                    android:layout_row="2"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist32"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="1"
                    android:layout_row="2"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

                <ExpandableListView
                    android:id="@+id/tablelist33"
                    android:layout_width="120dp"
                    android:layout_height="160dp"
                    android:layout_column="2"
                    android:layout_row="2"
                    android:backgroundTint="#000000"
                    android:focusable="false"
                    android:fontFamily="sans-serif-smallcaps"
                    android:foregroundTint="#000000"
                    android:textColor="#000000"
                    android:textSize="15dp" />

            </GridLayout>
        </ScrollView>

    </GridLayout>

</android.support.constraint.ConstraintLayout>
